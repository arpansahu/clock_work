celery -A clock_work.celery worker -l info
celery -A clock_work beat -l INFO
uvicorn clock_work.asgi:application --host 0.0.0.0 --port 8012
celery -A clock_work flower --port=8051

uvicorn clock_work.asgi:application --host 0.0.0.0 --port 8012 & celery -A clock_work.celery worker -l info & celery -A clock_work beat -l INFO & celery -A clock_work flower --port=8051

