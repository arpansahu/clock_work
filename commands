celery -A clock_work.celery worker -l info
celery -A clock_work beat -l INFO
daphne clock_work.asgi:application -b 0.0.0.0 --port 8012
celery -A clock_work flower --port=8051
daphne clock_work.asgi:application -b 0.0.0.0 --port 8012 & celery -A clock_work.celery worker -l info & celery -A clock_work beat -l INFO
